include inc/core/_config
include inc/core/_mixins
extends inc/core/_base
block title
  title= page.top.title
block prepend meta
  - var sitename = name
  - var keywords = page.top.keywords
  - var description = page.top.description
  - var ogpimage = page.top.ogpimage
  - var bodyClass = 'home'
block append meta
block body
  .l-container
    include inc/layouts/_header
    h1
      img(src='/docs/images/logo.svg', alt='Design Archives WordCamp Tokyo 2015')
    ul
      li
        a(href='')
          img(src='/docs/images/icon_fb.svg', alt='Share with Facebook')
      li
        a(href='')
          img(src='/docs/images/icon_tw.svg', alt='Tweet')
    include inc/modules/_list_repos
    include inc/layouts/_footer
block append scriptFooter
